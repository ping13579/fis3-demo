<div id="{{id}}" class="dateSelect">
    <div class="d-main">
        <div class="d-header">
            <a class="func-btn okBtn" href="javascript:void(0)">确定</a>
            <h1>{{title}}</h1>
            <a class="func-btn cancelBtn" href="javascript:void(0)">取消</a>
        </div>
        <div class="date-mark"></div>
        <div class="dateScroll {{type}}">
            <div id="YearStartScroll" class="scroll-item scroll-col-1">
                <ul></ul>
            </div>
            <div id="MonthStartScroll" class="scroll-item scroll-col-2">
                <ul></ul>
            </div>
            <div id="DayStartScroll" class="scroll-item scroll-col-3">
                <ul></ul>
            </div>
        </div>
    </div>
</div>'